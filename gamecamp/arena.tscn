[sub_resource type="GDScript" id="GDScript_cmmai"]
script/source = "extends Camera2D

var screen_shake_start = false
var shake_intensity = 0
var predef_loc


func _ready():
	Global.Camera = self

func _exit_tree():
	Global.Camera = null

func _process(delta):
	zoom = lerp(zoom, Vector2(1, 1), 0.3)

	if screen_shake_start == true:
		global_position += Vector2(randf_range(-shake_intensity, shake_intensity), randf_range(-shake_intensity, shake_intensity))

func screen_shake(intensity, time):
	zoom = Vector2(1, 1) - Vector2(intensity + 0.002, intensity * 0.02)
	shake_intensity = intensity
	$screenshake_timer.wait_time = time
	$screenshake_timer.start()
	screen_shake_start = true

func _on_screenshake_timer_timeout() -> void:
	predef_loc = Global.player.global_position
	global_position = predef_loc
	screen_shake_start = false
"

